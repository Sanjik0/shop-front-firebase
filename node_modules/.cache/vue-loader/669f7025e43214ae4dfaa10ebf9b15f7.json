urn this.$store.getters.info.bill / (this.rates['KZT'] / this.rates['USD'])\n            }\n        },\n        methods: {\n            getCurrency(currency) {\n                return Math.floor(this.base * this.rates[currency])\n            }\n        },\n        async created() {\n            const settings = await this.$store.dispatch('fetchSettings')\n            this.selectedColor = settings.selectedColor\n        }\n    }\n</script>"]}]}