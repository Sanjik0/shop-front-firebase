     <div class=\"card-content white-text\">\n                <span class=\"card-title\">{{'Currency_account' | localize}}</span>\n                <p \n                    v-for=\"cur of currencies\"\n                    :key=\"cur\"\n                    class=\"currency-line\">\n                <span>\n                    {{ getCurrency(cur) | currency(cur) }}\n                </span>\n                </p>\n        </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        props: ['rates'],\n        data: () => ({\n            currencies: ['USD','KZT','RUB'],\n            selectedColor: ''\n        }),\n        computed: {\n            base() {\n                return this.$store.getters.info.bill / (this.rates['KZT'] / this.rates['USD'])\n            }\n        },\n        methods: {\n            getCurrency(currency) {\n                return Math.floor(this.base * this.rates[currency])\n            }\n        },\n        async created() {\n            const settings = await this.$store.dispatch('fetchSettings')\n            this.selectedColor = settings.selectedColor\n        }\n    }\n</script>"],"sourceRoot":"src/components"}]}